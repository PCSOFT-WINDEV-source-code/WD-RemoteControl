  type_code : 1
  p_codes :
   -
     code : |1+
      PROCEDURE WIN_WindowAutomaticCheck(gnMode is int, gsMessage is string, gnDisplayDuration is int)
      
   -
     code : |1+
      // Modifies the title according to the mode
      SWITCH gnMode
      	CASE acForbidden : STC_Title = <§$0000§>
      	CASE acWarning : STC_Title = <§$0001§>
      	CASE acStop : STC_Title = <§$0002§>
      END
      
      // Displays the message
      STC_Message = gsMessage
      
      // Defines the time-out of the closing button
      DelayBeforeClosing(MyWindow, BTN_Close, gnDisplayDuration*100)
     type : 34
   -
     type : 180
     enabled : false
   -
     type : 230
     enabled : false
   -
     code : |1+
      // If the mode is "Stop", the application must be stopped
      IF gnMode = acStop THEN EndProgram()
      
